      <service-qname localpart="CompanyServicesService" namespaceURI="http://services.company.tqi.com.br/"/>
      <port-info>
         <wsdl-port localpart="CompanyServicesPort" namespaceURI="http://services.company.tqi.com.br/"/>
         <runtime/>
